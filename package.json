{
  "name": "branch-deck",
  "private": true,
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "test:rust": "cargo nextest run --workspace --exclude model-ai",
    "test:rust-doc": "cargo test --doc --workspace",
    "test-model-ai": "cargo test -p model-ai --release",
    "e2e": "playwright test",
    "e2e-up": "playwright test --update-snapshots",
    "e2e:regenerate": "cargo run --bin regenerate-templates -p test-server",
    "dev:test": "NUXT_PUBLIC_TEST_MODE=true nuxt dev",
    "test": "pnpm run \"/^test:.*/\"",
    "tauri": "tauri",
    "test-server:dev": "watchexec -w Cargo.toml -w Cargo.lock -w crates/test-server -w crates/git-ops -w crates/test-utils -w crates/branch-sync -r -- cargo run -p test-server",
    "lint:backend": "cargo fmt --all && cargo clippy --all --allow-dirty --fix",
    "lint:js": "eslint . --fix",
    "lint:typecheck": "nuxt typecheck",
    "lint:frontend": "pnpm run \"/^lint:(js|typecheck)$/\"",
    "lint": "pnpm run \"/^lint:.*/\""
  },
  "dependencies": {
    "@git-diff-view/vue": "0.0.30",
    "@nuxt/ui": "^3.3.0",
    "@nuxt/ui-pro": "^3.3.0",
    "@tanstack/vue-table": "^8.21.3",
    "@tauri-apps/api": "2.7.0",
    "@tauri-apps/plugin-log": "2.6.0",
    "@tauri-apps/plugin-opener": "2.4.0",
    "@tauri-apps/plugin-store": "2.3.0",
    "@tauri-apps/plugin-window-state": "2.4.0",
    "@vueuse/core": "13.6.0",
    "@vueuse/integrations": "^13.6.0",
    "reka-ui": "^2.4.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.60",
    "@nuxt/eslint": "1.7.1",
    "@playwright/test": "^1.54.2",
    "@stylistic/eslint-plugin": "5.2.2",
    "@tanstack/table-core": "8.21.3",
    "@tauri-apps/cli": "2.7.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "14.6.0",
    "eslint": "9.32.0",
    "eslint-plugin-vue": "10.4.0",
    "nuxt": "4.0.2",
    "tailwindcss": "4.1.11",
    "typescript": "5.9.2",
    "vite": "7.0.6",
    "vue": "^3.5.18",
    "vue-tsc": "3.0.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
