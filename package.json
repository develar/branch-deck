{
  "name": "branch-deck",
  "private": true,
  "version": "0.8.0",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "test:backend": "cargo nextest run --workspace --exclude model-ai",
    "test-model-ai": "cargo nextest run -p model-ai --release",
    "test-model-ai-up": "INSTA_UPDATE=always cargo nextest run -p model-ai --release",
    "e2e": "playwright test",
    "e2e-up": "playwright test --update-snapshots",
    "e2e:regenerate": "cargo run --bin regenerate-templates -p test-server",
    "dev:test": "NUXT_PUBLIC_TEST_MODE=true nuxt dev",
    "test": "pnpm run \"/^test:.*/\"",
    "tauri": "tauri",
    "test-server:dev": "watchexec -w Cargo.toml -w Cargo.lock -w crates/branch-integration -w crates/git-executor -w crates/git-ops -w crates/model-ai -w crates/model-core -w crates/sync-core -w crates/sync-test-utils -w crates/sync-types -w crates/test-server -w crates/test-utils -r -- cargo run -p test-server",
    "lint:backend": "cargo fmt --all && cargo clippy --all --allow-dirty --fix",
    "lint:js": "eslint . --fix",
    "lint:typecheck": "nuxt typecheck",
    "lint:frontend": "pnpm run \"/^lint:(js|typecheck)$/\"",
    "lint": "pnpm run \"/^lint:.*/\""
  },
  "dependencies": {
    "@git-diff-view/vue": "0.0.31",
    "@nuxt/ui": "4.0.0-alpha.0",
    "@tanstack/vue-table": "^8.21.3",
    "@tauri-apps/api": "2.8.0",
    "@tauri-apps/plugin-log": "2.6.0",
    "@tauri-apps/plugin-opener": "2.5.0",
    "@tauri-apps/plugin-store": "2.4.0",
    "@tauri-apps/plugin-window-state": "2.4.0",
    "@vueuse/core": "13.7.0",
    "@vueuse/integrations": "^13.7.0",
    "p-debounce": "^4.0.0",
    "reka-ui": "^2.4.1",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.64",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@playwright/test": "^1.55.0",
    "@stylistic/eslint-plugin": "5.2.3",
    "@tanstack/table-core": "8.21.3",
    "@tauri-apps/cli": "2.8.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "14.6.0",
    "eslint": "9.34.0",
    "eslint-plugin-vue": "10.4.0",
    "nuxt": "4.0.3",
    "tailwindcss": "4.1.12",
    "typescript": "5.9.2",
    "vite": "7.1.3",
    "vue": "^3.5.20",
    "vue-tsc": "3.0.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ],
    "overrides": {
      "zod": "^4.1.1"
    }
  }
}
