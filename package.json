{
  "name": "branch-deck",
  "private": true,
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "test:rust": "cargo test --workspace --exclude model-ai",
    "test-model-ai": "cargo test -p model-ai --release",
    "test:vitest": "vitest",
    "e2e": "playwright test",
    "e2e:regenerate": "cargo run --bin regenerate-templates -p test-server",
    "dev:test": "NUXT_PUBLIC_TEST_MODE=true nuxt dev",
    "test": "pnpm run \"/^test:.*/\"",
    "tauri": "tauri",
    "lint:rust": "cargo fmt --all && cargo clippy --all --allow-dirty --fix",
    "lint:js": "eslint . --fix",
    "lint:typecheck": "nuxt typecheck",
    "lint": "pnpm run \"/^lint:.*/\""
  },
  "dependencies": {
    "@git-diff-view/vue": "0.0.30",
    "@nuxt/ui": "^3.3.0",
    "@nuxt/ui-pro": "^3.3.0",
    "@pinia/nuxt": "^0.11.2",
    "@tanstack/vue-table": "^8.21.3",
    "@tauri-apps/api": "2.7.0",
    "@tauri-apps/plugin-log": "2.6.0",
    "@tauri-apps/plugin-opener": "2.4.0",
    "@tauri-apps/plugin-store": "2.3.0",
    "@tauri-apps/plugin-window-state": "2.4.0",
    "@vueuse/core": "13.5.0",
    "@vueuse/integrations": "^13.5.0",
    "focus-trap": "^7.6.5",
    "pinia": "^3.0.3",
    "reka-ui": "^2.4.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.7.1",
    "@nuxt/test-utils": "^3.19.2",
    "@playwright/test": "^1.54.1",
    "@stylistic/eslint-plugin": "5.2.2",
    "@tanstack/table-core": "8.21.3",
    "@tauri-apps/cli": "2.7.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "9.31.0",
    "eslint-plugin-vue": "10.3.0",
    "happy-dom": "^18.0.1",
    "nuxt": "4.0.1",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "vite": "7.0.6",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "3.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
